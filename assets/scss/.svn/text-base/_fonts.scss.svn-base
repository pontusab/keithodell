@font-face {
	font-family: 'CoopNew';
	src: url('../fonts/coop_new/coopnew-regular-webfont.eot');
	src: url('../fonts/coop_new/coopnew-regular-webfont.eot?#iefix') format('embedded-opentype'),
		 url('../fonts/coop_new/coopnew-regular-webfont.woff') format('woff'),
		 url('../fonts/coop_new/coopnew-regular-webfont.ttf') format('truetype'),
		 url('../fonts/coop_new/coopnew-regular-webfont.svg#CoopNew-Regular') format('svg');
	font-weight: normal;
	font-style: normal;
}

